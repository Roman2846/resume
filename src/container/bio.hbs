<div>

    <div>
        <ul>
        <li>
            <p><b>Name:</b> {{name}}</p>
            <p><b>Birthdate:</b> {{birthdate}}</p>
            <p><b>Birthplace:</b> {{birthplace}}</p>
            <p><b>Deathdate:</b> {{deathdate}}</p>
            <p><b>Deathplace:</b> {{deathplace}}</p>
            <p><b>Nationality:</b> {{nationality}}</p>
            <p><b>Occupation:</b> {{occupation}}</p>
        </li>
        </ul>
    </div>

    <br>

    <ul>
        <li>
        <p><b>Known_for:</b> {{known_for}}</p>
        </li>
     </ul>

    <br>

    <div>
        {{> title data="Educations:" big=true}}
        <ul>
            {{#each education}}
                <li>
                    <p><b>Degree:</b> {{degree}}</p>
                    <p><b>Field:</b> {{field}}</p>
                    <p><b>Institution:</b> {{institution}}</p>
                    <p><b>Year:</b> {{year}}</p>
                </li>
            {{/each}}
        </ul>
    </div>

    <br>

    <div>
        {{> title data="Notable_publications:" big=true}}
        <ul>
            {{#each notable_publications}}
                <li>
                    <p><b>Title:</b> {{title}}</p>
                    <p><b>Year:</b> {{year}}</p>
                    <p><b>Publisher:</b> {{publisher}}</p>
                </li>
            {{/each}}
        </ul>
    </div>

    <br>

    <div>
        {{> title data="Partners:" big=true}}
        <ul>
            {{#each partners}}
                <li>
                    <p><b>Name:</b> {{name}}</p>
                    <p><b>Relationship:</b> {{relationship}}</p>
                    <p><b>Years:</b> {{years}}</p>
                </li>
            {{/each}}
        </ul>
    </div>

    <br>

    <div>
        {{> title data="Awards:" big=true}}
        <ul>
            {{#each awards}}
                <li>
                    <p><b>Title:</b> {{title}}</p>
                    <p><b>Year:</b> {{year}}</p>
                    <p><b>Description:</b> {{description}}</p>
                </li>
            {{/each}}
        </ul>
    </div>

    <br>

        <ul>
            <li>
                <p><b>Influences:</b> {{influences}}</p>
            </li>
        </ul>

    <br>

        <ul>
            <li>
                <p><b>Influenced:</b> {{influenced}}</p>
            </li>
        </ul>

    <br>

        <ul>
            <li>
                <p><b>Quotes:</b> {{quotes}}</p>
            </li>
        </ul>

    <br>

    <div>
        {{> title data="Major_discoveries:" big=true}}
        <ul>
            {{#each major_discoveries}}
                <li>
                    <p><b>Title:</b> {{title}}</p>
                    <p><b>Year:</b> {{year}}</p>
                    <p><b>Description:</b> {{description}}</p>
                </li>
            {{/each}}
        </ul>
    </div>

    <br>

    <div>
        {{> title data="Contributions:" big=true}}
        <ul>
            {{#with contributions}}
                <li>
                    <p><b>Title:</b> {{title}}</p>
                    <p><b>Description:</b> {{description}}</p>
                    <p><b>Year:</b> {{year}}</p>
                    <p><b>Field:</b> {{field}}</p>
                </li>
            {{/with}}
        </ul>
    </div>
  
</div>